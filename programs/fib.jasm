; Simple JASM program to calculate the 10th Fibonacci number

start:
    MOV A, 0
    MOV B, 1
    MOV C, 10 ; counter

loop:
    MOV D, A      ; save old A
    ADD A, B       ; A = A + B (new fib number)
    MOV B, D      ; B = old A

    DEC C ; decrement counter
    JZ loop_end ; if counter is 0, we're done

    JMP loop ; otherwise, continue the loop

loop_end:
    PUT A, 0xC000 ; store the result to memory
    HALT
