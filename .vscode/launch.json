    "version": "0.2.0",
    "configurations": [
        {
            "name": "Jaide Debugger",
            "type": "debugpy",
            "request": "launch",
            "module": "jaide"
        },
        {
            "name": "JASM Debugger",
            "type": "debugpy",
            "request": "launch",
            "module": "jasm",
            "args": [
                "programs/graphics.jasm",
                "-o",
                "programs/bin/graphics.bin"
            ]
        }
    ]
}